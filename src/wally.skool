 $EC1E PUSH DE       ; }
 $EC1F LD E,A        ; { Put the flag in #REGde.
 $EC20 LD D,$00      ; }
 $EC22 LD HL,$EC0F   ; { Add the base pointer to the flags at #R$EC0F.
 $EC25 ADD HL,DE     ; }
 $EC26 LD A,(HL)     ; Get the flag.
 $EC27 POP DE        ; { Restore #REGhl and #REGde.
