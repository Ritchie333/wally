 $AC78 DEC E         ; Reduce the number of matches to find.
 $AC79 JP NZ,$AC76   ; Jump back if there are more entries to look for.
 $AC7C POP DE        ; { Restore #REGde and #REGbc and return.
 $AC7D POP BC        ; 
