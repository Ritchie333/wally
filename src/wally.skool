 $B2BB ADD A,$02     ; Add 2.
 $B2BD CP $F0        ; { If it's greater than #N$F0, set it to that.
 $B2BF JR C,$B2D5    ; 
 $B2C1 LD A,$F0      ; 
 $B2C3 JP $B2D5      ; }
; Move left.
*$B2C6 LD A,(IY-$3C) ; Get the direction flag. (#R$BCA3)
 $B2C9 OR A          ; Is it 0?
