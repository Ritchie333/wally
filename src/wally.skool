*$E3B4 LD A,$20      ;
 $E3B6 CALL $B7BA    ; Draw the graphic.
 $E3B9 INC E         ;
 $E3BA JR $E3AE      ;
*$E3BC ADD A,$08     ;
 $E3BE SRL A         ;
 $E3C0 LD B,A        ;
 $E3C1 LD A,L        ;
 $E3C2 CP $5C        ;
 $E3C4 JR NZ,$E3CA   ;
