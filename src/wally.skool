 $B2CD LD A,(IY+$05) ; Get the X co-ordinate.
 $B2D0 SUB $02       ; Subtract 2.
 $B2D2 JR NC,$B2D5   ; { If it's less than 0, set it to that.
 $B2D4 XOR A         ; }
*$B2D5 LD (IY+$05),A ; Set the new X co-ordinate
