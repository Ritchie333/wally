 $EBC8 SUB $04       ;
 $EBCA CP $F8        ; }
 $EBCC JR C,$EBDD    ; Jump forward if not.
 $EBCE INC HL        ; Point to the Y co-ordinate.
 $EBCF LD A,(IY+$0A) ; Get the Y co-ordinate.
