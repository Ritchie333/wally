 $AC60 LD HL,$AF10   ; Point #REGhl at the start of the co-ordinate tables. (#R$AF10)
 $AC63 ADD A,L       ; Add the offset to get the correct address.
 $AC64 JR NC,$AC67   ; { Increment the high byte if there was an overflow,
